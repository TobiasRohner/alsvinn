<?xml version="1.0" ?><fvm>
    <name>
        tecnoburgers
    </name>
    <platform>cuda</platform>
    <grid>
        <lowerCorner>-1 0 0</lowerCorner>
        <upperCorner>1 0 0</upperCorner>
        <dimension>4096 1 1</dimension>
    </grid>
    <boundary>periodic</boundary>
    <flux>tecno4</flux>
    <endTime>0.01</endTime>
    <equation>burgers</equation>
    <reconstruction>none</reconstruction>
    <cfl>0.1</cfl>
    <diffusion>
        <name>tecnoroe</name>
        <reconstruction>eno2</reconstruction>
    </diffusion>
    <integrator>rungekutta3</integrator>
    <initialData>
        <python>tecnoburgers.py</python>
    </initialData>
    <writer>
        <type>hdf5</type>
        <basename>tecnoburgers</basename>
        <numberOfSaves>10</numberOfSaves>
    </writer>
</fvm>