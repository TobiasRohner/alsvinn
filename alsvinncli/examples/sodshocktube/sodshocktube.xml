<fvm>
  <name>
    sodtube
  </name>
  <platform>cpu</platform>
  <grid>
    <lowerCorner>-5 0 0</lowerCorner>
    <upperCorner>5 0 0</upperCorner>
    <dimension>100 1 1</dimension>
  </grid>
  <boundary>neumann</boundary>
  <flux>tecno4</flux>
  <endTime>0.5</endTime>
  <equation>euler</equation>
  <reconstruction>none</reconstruction>
  <cfl>0.001</cfl>
  <integrator>rungekutta2</integrator>
    <equationParameters>
        <gamma>1.4</gamma>
    </equationParameters>
  <initialData>
    <python>sodshocktube.py</python>
  </initialData>
    <diffusion>
        <name>tecnoroe</name>
        <reconstruction>none</reconstruction>
    </diffusion>
  <writer>
    <type>hdf5</type>
    <basename>sodtube</basename>
    <numberOfSaves>10</numberOfSaves>
  </writer>
</fvm>
