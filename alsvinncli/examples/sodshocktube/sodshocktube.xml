<fvm>
  <name>
    sodtube
  </name>
  <platform>cpu</platform>
  <grid>
    <lowerCorner>0 0 0</lowerCorner>
    <upperCorner>2 0 0</upperCorner>
    <dimension>32 1 1</dimension>
  </grid>
  <boundary>neumann</boundary>
  <flux>tecno1</flux>
  <endTime>0.5</endTime>
  <equation>euler</equation>
  <reconstruction>none</reconstruction>
  <cfl>0.1</cfl>
  <integrator>auto</integrator>
  <initialData>
    <python>sodshocktube.py</python>
  </initialData>
    <diffusion>
        <name>tecnoroe</name>
        <reconstruction>none</reconstruction>
    </diffusion>
  <writer>
    <type>hdf5</type>
    <basename>sodtube</basename>
    <numberOfSaves>10</numberOfSaves>
  </writer>
</fvm>
