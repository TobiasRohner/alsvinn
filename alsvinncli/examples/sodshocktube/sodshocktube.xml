<fvm>
  <name>
    sodtube
  </name>
  <platform>cpu</platform>
  <grid>
    <lowerCorner>0 0 0</lowerCorner>
    <upperCorner>2 0 0</upperCorner>
    <dimension>128 1 1</dimension>
  </grid>
  <boundary>neumann</boundary>
  <flux>HLL</flux>
  <endTime>0.5</endTime>
  <equation>euler</equation>
  <reconstruction>eno2</reconstruction>
  <cfl>auto</cfl>
  <integrator>rungekutta3</integrator>
  <initialData>
    <python>sodshocktube.py</python>
  </initialData>
  <writer>
    <type>hdf5</type>
    <basename>sodtube</basename>
    <numberOfSaves>10</numberOfSaves>
  </writer>
</fvm>
