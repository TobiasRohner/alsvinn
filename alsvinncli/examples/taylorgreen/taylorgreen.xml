<config>
<fvm>
  <name>
    taylorgreen
  </name>
  <platform>cuda</platform>
  <grid>
    <lowerCorner>0 0 0</lowerCorner>
    <upperCorner>1 1 1</upperCorner>
    <dimension>64 64 64</dimension>
  </grid>
  <boundary>periodic</boundary>
  <flux>HLL3</flux>
  <endTime>2</endTime>
  <equation>euler3</equation>
  <reconstruction>none</reconstruction>
  <cfl>auto</cfl>
  <integrator>auto</integrator>
  <initialData>
    <python>taylorgreen.py</python>
  </initialData>
  <writer>
    <type>netcdf</type>
    <basename>tg</basename>
    <numberOfSaves>1</numberOfSaves>
  </writer>
</fvm>
<uq>
<samples>1</samples>
<generator>auto</generator>
<parameters>
  <parameter>
    <name>a</name>
    <length>40</length>
    <type>uniform</type>
  </parameter>
</parameters>
<stats>
<stat>
  <name>
    meanvar
  </name>
  <numberOfSaves>1</numberOfSaves>
  <writer>
    <type>netcdf</type>
    <basename>kh</basename>
  </writer>
  </stat>
</stats>
</uq>
</config>
