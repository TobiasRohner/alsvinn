<config>
<fvm>
    <name>
        shockvortex_uniform
    </name>
    <platform>cpu</platform>
    <grid>
        <lowerCorner>0 0 0</lowerCorner>
        <upperCorner>1 1 0</upperCorner>
        <dimension>1024 1024 1</dimension>
    </grid>
    <boundary>neumann</boundary>
    <flux>HLL3</flux>
    <endTime>0.35</endTime>
    <equation>euler2</equation>
    <equationParameters>
        <gamma>1.66666666666667</gamma>
    </equationParameters>
    <reconstruction>weno2</reconstruction>
    <cfl>auto</cfl>
    <integrator>auto</integrator>
    <initialData>
        <python>shockvortex_uniform.py</python>
	<parameters>
	  <parameter>
            <name>a</name>
            <length>40</length>
            <values><value>0</value></values>
	  </parameter>
	</parameters>
    </initialData>
    <diffusion>
        <name>none</name>
        <reconstruction>none</reconstruction>
    </diffusion>
    <writer>
        <type>netcdf</type>
        <basename>shockvortex_uniform</basename>
        <numberOfSaves>1</numberOfSaves>
    </writer>
</fvm>
<uq>
  <samples>1024</samples>
  <generator>auto</generator>
  <parameters>
    <parameter>
      <name>X</name>
      <length>1</length>
      <type>uniform</type>
    </parameter>
  </parameters>
  <stats>
  </stats>
</uq>
</config>
